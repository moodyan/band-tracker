@{
  Layout="Shared/_Layout.cshtml";
}
@if (@Model.Count == 0)
{
  <p>No venues have been added yet!</p>
}
else
{
  <h1>Here are the venues we track:</h1>
  <ul>
    @foreach (var venue in Model)
    {
      <li><a href='/venue/@venue.GetId()'>@venue.GetVenueName()</a></li>
    }
  </ul>

}
<br>
<p><a href="/venues/new">Track a new venue</a></p>
